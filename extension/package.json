{
  "name": "1loc",
  "displayName": "1loc",
  "description": "Code snippets for JavaScript utilities in one line of code (loc)",
  "version": "1.0.1",
  "publisher": "codeshifu",
  "engines": {
    "vscode": "^1.62.0"
  },
  "license": "MIT",
  "categories": [
    "Snippets"
  ],
  "icon": "icon.png",
  "keywords": [
    "1loc",
    "JavaScript",
    "TypeScript",
    "node",
    "snippet"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/1loc.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/1loc.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/1loc-ts.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/1loc-ts.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codeshifu/1loc-vscode"
  }
}
