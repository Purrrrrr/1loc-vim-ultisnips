snippet onelocisIE "Detect Internet Explorer browser"
/* Detect Internet Explorer browser */
const isIE = !!document.documentMode;
endsnippet

snippet onelocisMacBrowser "Detect macOS browser"
/* Detect macOS browser */
const isMacBrowser = /Mac|iPod|iPhone|iPad/.test(navigator.platform);
endsnippet

snippet onelocsiblings "Get all siblings of an element"
/* Get all siblings of an element */
const siblings = (ele) => [].slice.call(ele.parentNode.children).filter((child) => child !== ele);
endsnippet

snippet onelocgetPosition "Get the position of an element relative to the document"
