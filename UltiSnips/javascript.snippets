snippet oneloccountBy "Count by the properties of an array of objects"
/* Count by the properties of an array of objects */
const countBy = (arr, prop) => arr.reduce((prev, curr) => ((prev[curr[prop]] = ++prev[curr[prop]] || 1), prev), {});
endsnippet

snippet oneloccountOccurrences "Count the occurrences of a value in an array"
/* Count the occurrences of a value in an array */
const countOccurrences = (arr, val) => arr.reduce((a, v) => (v === val ? a + 1 : a), 0)
// Similar 👇🏼,  const countOccurrences = (arr, val) => arr.filter((item) => item === val).length;
endsnippet

snippet oneloccountOccurrences "Count the occurrences of array elements"
/* Count the occurrences of array elements */
const countOccurrences = (arr) => arr.reduce((prev, curr) => ((prev[curr] = ++prev[curr] || 1), prev), {});
endsnippet

snippet onelocaccumulate "Create an array of cumulative sum"
/* Create an array of cumulative sum */
const accumulate = (arr) => arr.map(((sum) => (value) => (sum += value))(0))
// Similar 👇🏼,  const accumulate = (arr) => arr.reduce((a, b, i) => (i === 0 ? [b] : [...a, b + a[i - 1]]), [0]);
endsnippet

snippet onelocrange "Create an array of numbers in the given range"
