endsnippet

snippet onelocpartition "Partition an array based on a condition"
/* Partition an array based on a condition */
const partition = (arr, criteria) => arr.reduce((acc, i) => (acc[criteria(i) ? 0 : 1].push(i), acc), [[], []]);
endsnippet

snippet onelocremoveDuplicate "Remove duplicate values in an array"
/* Remove duplicate values in an array */
const removeDuplicate = (arr) => arr.filter((i) => arr.indexOf(i) === arr.lastIndexOf(i));
endsnippet

snippet onelocremoveFalsy "Remove falsy values from array"
/* Remove falsy values from array */
const removeFalsy = (arr) => arr.filter(Boolean);
endsnippet

snippet onelocrepeat "Repeat an array"
