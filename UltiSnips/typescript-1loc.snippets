snippet 1locxor "Logical xor operator"
/* Logical xor operator */
const xor = (a, b) => (a && !b) || (!a && b)
// Similar 👇🏼,  const xor = (a, b) => !(!a && !b) && !(a && b),  const xor = (a, b) => Boolean(!a ^ !b);
