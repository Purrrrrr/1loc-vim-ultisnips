const isString = (value: any): boolean => Object.prototype.toString.call(value) === '[object String]';
endsnippet

snippet 1locisObject "Check if a value is an object"
/* Check if a value is an object */
const isObject = (v: any): boolean => v !== null && typeof v === 'object';
endsnippet

snippet 1locisBase32 "Check if a value is base32 encoded"
/* Check if a value is base32 encoded */
const isBase32 = (value: string): boolean => value.length % 8 === 0 && /^[A-Z2-7]+=*$/.test(value);
endsnippet

snippet 1locisBase58 "Check if a value is base58 encoded"
/* Check if a value is base58 encoded */
const isBase58 = (value: string): boolean => /^[A-HJ-NP-Za-km-z1-9]*$/.test(value);
endsnippet

snippet 1locisBase64 "Check if a value is base64 encoded"
/* Check if a value is base64 encoded */
const isBase64 = (value: string): boolean => /^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{4})$/.test(value);
endsnippet

snippet 1locisNil "Check if a value is nil"
/* Check if a value is nil */
const isNil = (value: any): boolean => value == null;
endsnippet

snippet 1locisLeapYear "Check if a year is leap year"
/* Check if a year is leap year */
const isLeapYear = (year: number): boolean => (year % 4 === 0 && year % 100 !== 0) || year % 400 === 0
// Similar 👇🏼,  const isLeapYear = (year: number): boolean => new Date(year, 1, 29).getDate() === 29;
endsnippet

snippet 1locisEqual "Check if all array elements are equal to a given value"
/* Check if all array elements are equal to a given value */
const isEqual = <T,_>(arr: T[], value: T): boolean => arr.every((item) => item === value)
// Similar 👇🏼,  const isEqual = <T,_>(arr: T[], value: T): boolean => !arr.some((item) => item !== value);
endsnippet

snippet 1locareEqual "Check if all items in an array are equal"
/* Check if all items in an array are equal */
const areEqual = <T,_>(arr: T[]): boolean => arr.length > 0 && arr.every((item) => item === arr[0])
// Similar 👇🏼,  const areEqual = <T,_>(arr: T[]): boolean => new Set(arr).size === 1;
endsnippet

snippet 1loccontains "Check if an array contains a value matching some criterias"
/* Check if an array contains a value matching some criterias */
const contains = <T,_>(arr: T[], criteria: (a: T) => boolean): boolean => arr.some((v) => criteria(v))
// Similar 👇🏼,  const contains = <T,_>(arr: T[], criteria: (a: T) => boolean): boolean => arr.some(criteria),  const contains = <T,_>(arr: T[], criteria: (a: T) => boolean): boolean => arr.filter(criteria).length > 0;
endsnippet

snippet 1locisNotEmpty "Check if an array is not empty"
/* Check if an array is not empty */
const isNotEmpty = (arr: any): boolean => Array.isArray(arr) && Object.keys(arr).length > 0;
endsnippet

snippet 1locisSubset "Check if an array is subset of other array"
/* Check if an array is subset of other array */
const isSubset = <T,_>(a: T[], b: T[]): boolean => new Set(b).size === new Set(b.concat(a)).size
// Similar 👇🏼,  const isSubset = <T,_>(a: T[], b: T[]): boolean => b.join('|').includes(a.join('|'));
endsnippet

snippet 1locisPromise "Check if an object is a Promise"
/* Check if an object is a Promise */
const isPromise = (obj: any): boolean => !!obj && (typeof obj === 'object' || typeof obj === 'function') && typeof obj.then === 'function';
endsnippet

snippet 1locisArray "Check if an object is an array"
/* Check if an object is an array */
const isArray = (obj: any): boolean => Array.isArray(obj);
endsnippet

snippet 1locisEmpty "Check if an object is empty"
/* Check if an object is empty */
const isEmpty = (obj: object): boolean => Reflect.ownKeys(obj).length === 0 && obj.constructor === Object
// Similar 👇🏼,  const isEmpty = (obj: object): boolean => JSON.stringify(obj) === '{\}';
endsnippet

snippet 1locisValidDate "Validate a Gregorian date"
/* Validate a Gregorian date */
const isValidDate = (m: number, d: number, y: number): boolean => 0 <= m && m <= 11 && 0 < y && y < 32768 && 0 < d && d <= new Date(y, m, 0).getDate();
endsnippet